        "@sveltejs/kit": "^1.30.3",
        "@tailwindcss/postcss": "^4.0.4",
        "@typescript-eslint/eslint-plugin": "^6.19.1",
        "@typescript-eslint/parser": "^6.19.1",
        "autoprefixer": "^10.4.20",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-svelte": "^2.35.1",
        "postcss": "^8.4.35",
        "prettier": "^3.2.4",
        "prettier-plugin-svelte": "^3.1.2",
        "sass": "^1.84.0",
        "svelte": "^4.2.9",
        "svelte-check": "^3.6.3",
        "tailwindcss": "^3.4.1",
        "tslib": "^2.6.2",
        "typescript": "^5.3.3",
